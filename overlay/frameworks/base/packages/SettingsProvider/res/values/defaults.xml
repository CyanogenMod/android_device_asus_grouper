<resources>
    <!-- 2 minutes for default screen timeout -->
    <integer name="def_screen_off_timeout">120000</integer>
    <!-- Set the automatic brightness mode on by default -->
    <bool name="def_screen_brightness_automatic_mode">true</bool>
    <!-- This device only uses WiFi network -->
    <bool name="def_wifi_on">true</bool>
    <!-- 0 == default, 1 == never while plugged, 2 == never -->
    <integer name="def_wifi_sleep_policy">2</integer>
</resources>
